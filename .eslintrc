{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allowAfterThis": true,
        "allowAfterSuper": true
      }
    ],
    "jsx-a11y/no-static-element-interactions": "off",
    "no-restricted-syntax": ["error", "WithStatement"],
    "react/prop-types": "off"
  }
}
